#include <iostream>
using namespace std;

class point {
	int x, y;
public:
	point( int a, int b ) {
		x = a;
		y = b;
	}
	void set ( int a, int b ) {
		x = a;
		y = b;
	}
	int getx() {
		return x;
	}
	int gety() {
		return y;
	}
        void print() {
		cout << "point " << x << " " << y << endl;
};

class line {
	point s, e;
public:
	line ( int a1, int b1, int a2, int b2 ) {
		s.set( a1, b1 );
		s.set( a2, b2 );
	}
	void print() {
		cout << "line start " << s.getx() << " " << s.gety() << " end " << e.getx() << " " << e.gety()
		     << endl;
};

int main() {
	point p;
	p.set( 3, 2 );
	line l( 0, 0, 1, 9 );
	p.print();
	l.print();
	return 0;
}
